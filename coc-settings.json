{
  "workspace.ignoredFolders": ["$HOME", "$HOME/.cargo/**", "$HOME/.rustup/**"],
  "html-css-support.enabledLanguages": [
    "html",
    "vue",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "suggest.noselect": true,
  "coc.preferences.extensionUpdateCheck": "never",
  "javascript.suggest.enabled": true,
  "javascript.validate.enable": true,
  "typescript.validate.enable": true,
  "typescript.autoClosingTags": true,
  "typescript.referencesCodeLens.showOnAllFunctions": true,
  "javascript.referencesCodeLens.showOnAllFunctions": true,
  "prettier.semi": true,
  "prettier.singleQuote": true,
  "yank.highlight.enable": false,
  "yank.enableCompletion": false,
  "tailwindCSS.emmetCompletions": true,
  "tailwindCSS.includeLanguages": {
    "liquid": "html"
  },
  "tailwindCSS.headwind.removeDuplicates": false,
  "hover.floatConfig": {
    "border": true,
    "rounded": true,
    "focusable": true
  },
  "suggest.floatConfig": {
    "border": true,
    "rounded": true
  },
  "diagnostic.floatConfig": {
    "border": true,
    "rounded": false
  },
  "signature.floatConfig": {
    "border": true,
    "rounded": true
  },
  "git.floatConfig": {
    "border": true,
    "rounded": true
  },
  "suggest.pumFloatConfig": {
    "border": true,
    "rounded": true
  },
  "outline.autoPreview": true,
  "list.signOffset": 1,
  "list.alignColumns": true,
  "list.floatPreview": true,
  "tsserver.trace.server": "messages",
  "git.showCommitInFloating": true,
  "git.addGBlameToVirtualText": true,
  "git.addGBlameToBufferVar": true,
  "git.enableGutters": true,
  "suggest.triggerAfterInsertEnter": true,
  "suggest.detailField": "preview",
  "javascript.referencesCodeLens.enabled": true
}
